    <script>
     
        // Store input values in cookies
        function storeData() {
            var inputs = document.querySelectorAll('input[type="text"], textarea, select');
            for (var i = 0; i < inputs.length; i++) {
                var input = inputs[i];
                document.cookie = input.id + '=' + encodeURIComponent(input.value) + '; expires=Fri, 31 Dec 9999 23:59:59 GMT';
            }
        }

        // Retrieve and display stored input values
        function displayStoredData() {
            var data = '';
            var cookies = document.cookie.split(';');
            for (var i = 0; i < cookies.length; i++) {
                var cookie = cookies[i].trim();
                if (cookie !== '') {
                    var cookieParts = cookie.split('=');
                    var inputId = cookieParts[0];
                    var encodedValue = cookieParts[1];
                    var value = decodeURIComponent(encodedValue);
                    data += inputId + ': ' + value + '\n';
                }
            }
            alert(data);
        }

        // Call loadData on page load
        window.onload = loadData;
    function loadVideo() {
        const input = document.getElementById('video-url-input');
        const videoPlayer = document.getElementById('video-player');
        const videoSource = document.getElementById('video-source');
    
        // Get the link from input and modify it for direct access
        let url = input.value.trim();
        url = url.replace("www.dropbox.com", "dl.dropboxusercontent.com").replace("dl=0", "raw=1");
    
        // Update the source of the video player
        videoSource.src = url;
        videoPlayer.load();  // Load the new video source
    
        // Optionally, handle loading errors
        videoPlayer.onerror = () => {
            console.error('Failed to load video.');
            alert('Video could not be loaded. Please check the link and try again.');
        };
    }
    
    // Optional: Automatically load video on page load if needed
    // window.onload = loadVideo;
    </script>
</body>
</html>
